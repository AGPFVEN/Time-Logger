    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_interface",
            "cwd": "flutter_interface",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_interface (profile mode)",
            "cwd": "flutter_interface",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_interface (release mode)",
            "cwd": "flutter_interface",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Google Cloud Functions (backend)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/backend/cmd/main.go",
            "cwd": "${workspaceFolder}/backend",
            "env": {
                "FUNCTION_TARGET": "HelloWorld",
                "LOCAL_ONLY": "true"
            }
        }
    ]
}